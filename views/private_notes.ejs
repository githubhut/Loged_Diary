<h1>Private Notes</h1>
    <form action="/notes/private" method="post">
        <textarea name="note" id="note" cols="30" rows="10"></textarea>
        <button type="submit">Add Private Note</button>
    </form>
    <ul>
        <% privateNotes.forEach(note => { %>
            <li><%= note %></li>
        <% }); %>
    </ul>